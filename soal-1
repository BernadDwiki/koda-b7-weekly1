let kata = "kasur ini rusak"
kata = kata.toLowerCase()
let palindrom ;
let tengah = (kata.length-1)/2
for(let i = 0; i<tengah;i++){
    if (kata[i] === kata[kata.length-(i+1)]){
        palindrom = true
    } else{
        palindrom = false
        break
    }
}

if (palindrom == true ){
    console.log("palindrom")
}else if (palindrom == false ) {
    console.log("bukan palindrom ")
}
